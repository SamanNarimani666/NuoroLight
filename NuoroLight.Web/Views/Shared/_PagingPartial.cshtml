@model NuoroLight.Domain.ViewModels.Paging.BasePaging
<div class="blog_pagination_section jb_cover">
    <ul>
        @if (Model.StartPage < Model.PageId)
        {
            <li class="btn">
                <a class="prev" onclick="FillPageId(@(Model.PageId - 1))"> <i class="flaticon-left-arrow"></i></a>
            </li>
        }
        @for (int i = Model.StartPage; i <= Model.EndPage; i++)
        {
            <li class="@(Model.PageId == i ? "third_pagger" : "d-block d-sm-block d-md-block d-lg-block")"><a  onclick="FillPageId(@i)" href="#">@i</a></li>
        }
        @if (Model.EndPage > Model.PageId)
        {
            <li class="btn">
                <a class="btn" class="next" onclick="FillPageId(@(Model.PageId + 1))"><i class="flaticon-right-arrow"></i></a>
            </li>
        }
    </ul>
</div>